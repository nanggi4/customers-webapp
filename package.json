    "build-webpack-customers-userconsole-dev453": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --env PRODUCT=customers --env STAGE=dev453 --mode development --config webpack.dev.js",
    "upload-bucket-customers-userconsole-dev453": "npm run build-webpack-customers-userconsole-dev453 && aws s3 cp ./dist-dev453 s3://userconsole.datamplan.com-customers-dev453/ --recursive --exclude \"*\" --include \"*.html\" --include \"*.js\" --include \"*.map\" --include \"*.css\" --include \"*.ico\" --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build-webpack-customers-userconsole-dev522": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --env PRODUCT=customers --env STAGE=dev522 --mode development --config webpack.dev.js",
    "upload-bucket-customers-userconsole-dev522": "npm run build-webpack-customers-userconsole-dev522 && aws s3 cp ./dist-dev522 s3://userconsole.datamplan.com-customers-dev522/ --recursive --exclude \"*\" --include \"*.html\" --include \"*.js\" --include \"*.map\" --include \"*.css\" --include \"*.ico\" --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers"
  },
