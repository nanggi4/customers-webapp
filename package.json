  "scripts": {
    "update-userconsole-webapp-builder": "rsync -avr --exclude='.git' ~/environment/userconsole-webapp-builder ./node_modules/@datamplan/",
    "webpack-dev-server": "npx webpack serve --mode development",
    "upload-bucket-customers-userconsole-lib": "aws s3 cp ./lib s3://cdn.userconsole.datamplan.com/ --recursive --exclude \"*\" --include \"*.html\" --include \"*.js\" --include \"*.map\" --include \"*.css\" --include \"*.ico\" --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build-webpack-customers-userconsole-prod": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --env PRODUCT=customers --env STAGE=prod --mode production --config webpack.prod.js",
    "upload-bucket-customers-userconsole-prod": "npm run build-webpack-customers-userconsole-prod && aws s3 cp ./dist s3://userconsole.datamplan.com-customers/ --recursive --exclude \"*\" --include \"*.html\" --include \"*.js\" --include \"*.map\" --include \"*.css\" --include \"*.ico\" --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build-webpack-customers-userconsole-dev342": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --env PRODUCT=customers --env STAGE=dev342 --mode development --config webpack.dev.js",
    "upload-bucket-customers-userconsole-dev342": "npm run build-webpack-customers-userconsole-dev342 && aws s3 cp ./dist-dev342 s3://userconsole.datamplan.com-customers-dev342/ --recursive --exclude \"*\" --include \"*.html\" --include \"*.js\" --include \"*.map\" --include \"*.css\" --include \"*.ico\" --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
